{
  "name": "lakovna-root",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "back",
    "front"
  ],
  "scripts": {
    "clean": "node scripts/clean.js",
    "build": "npm run build -w back && npm run build -w front",
    "dev": "node scripts/dev.js",
    "start": "node scripts/start-both.js",
    "prismag": "prisma generate --schema=./database/schema.prisma",
    "prismam": "prisma migrate dev --name init --schema=./database/schema.prisma",
    "prismas": "prisma db seed --schema=./database/schema.prisma",
    "prismac": "tsx scripts/check-prisma-types.ts --schema=./database/schema.prisma",
    "ssot": "npx tsx scripts/check-ssot-consistency.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "prisma": "^6.14.0",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "socket.io": "^4.7.5",
    "framer-motion": "^11.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.22.3",
    "recharts": "^2.12.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.7",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "nodemon": "^3.1.0",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vite": "^7.1.3"
  }
}